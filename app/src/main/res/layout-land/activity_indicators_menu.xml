        android:paddingEnd="20dp"
        app:layout_constraintBottom_toTopOf="@+id/lbl_TitleIndicatorLight"
        app:layout_constraintEnd_toEndOf="@+id/vw_IndicatorLight"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/vw_IndicatorLight"
