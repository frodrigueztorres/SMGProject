        android:text="@string/btnback"
        android:textAlignment="center"
        android:textColor="@android:color/white"
        app:layout_constraintEnd_toEndOf="@+id/btn_ImportIndicators"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/btn_ImportIndicators"
        app:layout_constraintTop_toBottomOf="@+id/txt_DescIndicators" />

    <Button
        android:id="@+id/btn_ImportIndicators"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginEnd="92dp"
        android:layout_marginBottom="188dp"
        android:background="@drawable/btn_background"
        android:text="@string/btnimport"
        android:textAlignment="center"
        android:textColor="@android:color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
