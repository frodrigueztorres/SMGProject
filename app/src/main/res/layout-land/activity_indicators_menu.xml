        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/lbl_TitleIndicatorTemp"
        app:layout_constraintEnd_toEndOf="@+id/vw_IndicatorTemp"
        app:layout_constraintHorizontal_bias="0.461"
        app:layout_constraintStart_toStartOf="@+id/vw_IndicatorTemp"
        app:layout_constraintTop_toTopOf="@+id/prb_IndicatorTemp"
        app:layout_constraintVertical_bias="0.882" />

    <TextView
        android:id="@+id/lbl_TitleIndicatorTemp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:text="@string/titletemp"
        android:textAlignment="center"
        android:textColor="@android:color/white"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/vw_IndicatorTemp"
        app:layout_constraintEnd_toEndOf="@+id/prb_IndicatorTemp"
