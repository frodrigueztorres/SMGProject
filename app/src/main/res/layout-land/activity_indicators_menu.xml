        android:paddingStart="20dp"
        android:paddingEnd="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/lbl_TitleIndicatorHS"
        app:layout_constraintEnd_toEndOf="@+id/vw_IndicatorHS"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/vw_IndicatorHS"
        app:layout_constraintTop_toTopOf="@+id/vw_IndicatorHS"
        app:layout_constraintVertical_bias="0.247" />

    <TextView
        android:id="@+id/lbl_TitleIndicatorHS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="@string/titlehs"
        android:textAlignment="center"
        android:textColor="@android:color/white"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/vw_IndicatorHS"
        app:layout_constraintEnd_toEndOf="@+id/vw_IndicatorHS"
        app:layout_constraintStart_toStartOf="@+id/vw_IndicatorHS" />

