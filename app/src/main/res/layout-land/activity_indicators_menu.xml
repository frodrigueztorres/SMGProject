    <TextView
        android:id="@+id/lbl_IndicatorHS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/indicatorbad"
        android:textColor="@android:color/white"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/vw_IndicatorHS"
        app:layout_constraintStart_toStartOf="@+id/vw_IndicatorHS"
        app:layout_constraintTop_toBottomOf="@+id/prb_IndicatorHS" />

    <ProgressBar
        android:id="@+id/prb_IndicatorHS"
